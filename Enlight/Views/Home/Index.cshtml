<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@*@ViewBag.Title*@</title>
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/progress-bar.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/flexslider.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/bselect.min.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" type="text/css" />
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300' rel='stylesheet' type='text/css'>
</head>
<body>
    <nav class="sticky no-display navbar navbar-default">
        <div class="container">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/"><img class="logo" src="~/Content/images/logo.png" href="/" /></a>
                </div>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li id="home-link" onclick="home(this)"><a href="javascript:void(0)">Home</a></li>
                    <li id="services-link" onclick="services(this)"><a href="javascript:void(0)">Services</a></li>
                    <li id="stream-link" onclick="stream(this)"><a href="javascript:void(0)">Stream</a></li>
                    <li id="clients-link" onclick="clients(this)"><a href="javascript:void(0)">Clients</a></li>
                    <li id="team-link" onclick="team(this)"><a href="javascript:void(0)">Team</a></li>
                    <li id="contact-link" onclick="contact(this)"><a href="javascript:void(0)">Contact</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <nav class="main-header navbar navbar-default">
        <div class="container">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse1" aria-expanded="false">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/"><img class="logo" src="~/Content/images/logo.png" href="/" /></a>
                </div>
            </div>
            <div class="navbar-collapse collapse" id="bs-example-navbar-collapse1">
                <ul class="nav navbar-nav navbar-right">
                    <li id="home-link" onclick="home(this)"><a href="javascript:void(0)">Home</a></li>
                    <li id="services-link" onclick="services(this)"><a href="javascript:void(0)">Services</a></li>
                    <li id="stream-link" onclick="stream(this)"><a href="javascript:void(0)">Stream</a></li>
                    <li id="clients-link" onclick="clients(this)"><a href="javascript:void(0)">Clients</a></li>
                    <li id="team-link" onclick="team(this)"><a href="javascript:void(0)">Team</a></li>
                    <li id="contact-link" onclick="contact(this)"><a href="javascript:void(0)">Contact</a></li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="body-content">
        <div ng-app="AngularCustomJS">
            <div ng-view></div>
        </div>
        @*@RenderBody()*@
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <p class="pull-left visible-md visible-lg">EMAIL US AT INFO@ENLIGHTWORLD.COM OR CALL US ON 19991</p>
                        <p class="pull-right visible-md visible-lg">COPYRIGHT ENLIGHT ADVISORY 2016, ALL RIGHTS RESERVED</p>
                        <p class="hidden-md hidden-lg">EMAIL US AT INFO@ENLIGHTWORLD.COM OR CALL US ON 19991</p>
                        <p class="hidden-md hidden-lg">COPYRIGHT ENLIGHT ADVISORY 2016, ALL RIGHTS RESERVED</p>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.15.0/jquery.validate.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.3/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.3/angular-route.min.js"></script>
    <script src="~/scripts/angular-validate.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.flexslider-min.js"></script>
    <script src="~/scripts/bselect.min.js"></script>
    <script src="~/scripts/jquery.animateNumber.min.js"></script>
    <script src="~/scripts/jquery.visible.min.js"></script>
    <script src="~/scripts/angular-sanitize.min.js"></script>
    @Scripts.Render("~/bundles/AngularCustomJS")
</body>
</html>